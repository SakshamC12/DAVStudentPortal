{
  "name": "dav-student-portal",
  "version": "1.0.0",
  "description": "Student Portal for DAV College - Check marks using Student ID and DOB",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "keywords": [
    "student",
    "portal",
    "marks",
    "dav",
    "college"
  ],
  "author": "DAV College",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.2",
    "antd": "^5.26.4",
    "xlsx": "^0.18.5"
  }
}
